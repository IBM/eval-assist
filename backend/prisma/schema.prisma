// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StoredUseCase {
  id      Int    @id @default(autoincrement())
  user_id Int
  content Json
  name    String @unique

  @@map("stored_use_case")
}

model AppUser {
  id    Int    @id @default(autoincrement())
  email String
}
